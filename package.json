    "typescript": "5.0.4",
    "vitest": "0.31.0",
    "zod": "3.21.4"
  },
  "peerDependencies": {
    "cross-fetch": ">= 3.1.5",
    "zod": "3.21.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": false
    },
    "cross-fetch": {
      "optional": false
    }
