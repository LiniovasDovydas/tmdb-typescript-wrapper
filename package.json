{
  "name": "tmdb-typescript-wrapper",
  "type": "module",
  "version": "1.0.0",
  "description": "TypeScript wrapper for The Movie Database API",
  "main": "./lib/index.mjs",
  "module": "./lib/index.mjs",
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch"
  },
  "keywords": [
    "tmdb",
    "typescript"
  ],
  "author": "Dovydas Liniovas <liniovas.dovydas@gmail.com>",
  "license": "MIT",
  "exports": {
    "default": "./lib/index.cjs",
    "import": "./lib/index.mjs",
    "require": "./lib/index.cjs",
    "node": "./lib/index.cjs"
  },
  "devDependencies": {
    "axios": "^1.3.4",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "zod": "^3.20.6"
  },
  "peerDependencies": {
    "zod": ">= 3.20.6",
    "axios": ">= 1.3.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": false
    }
  }
}
