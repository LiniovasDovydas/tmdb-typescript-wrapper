{
  "name": "tmdb-typescript-wrapper",
  "type": "module",
  "version": "1.0.0",
  "description": "TypeScript wrapper for The Movie Database API",
  "main": "./lib/index.mjs",
  "module": "./lib/index.mjs",
  "repository": "https://github.com/LiniovasDovydas/tmdb-typescript-wrapper.git",
  "bugs": {
    "url": "https://github.com/LiniovasDovydas/tmdb-typescript-wrapper/issues",
    "email": "liniovas.dovydas@gmail.com"
  },
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch"
  },
  "keywords": [
    "tmdb",
    "typescript"
  ],
  "author": "Dovydas Liniovas",
  "license": "MIT",
  "exports": {
    "default": "./lib/index.cjs",
    "import": "./lib/index.mjs",
    "require": "./lib/index.cjs",
    "node": "./lib/index.cjs"
  },
  "devDependencies": {
    "cross-fetch": "^3.1.5",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "zod": "^3.20.6"
  },
  "peerDependencies": {
    "cross-fetch": ">= 3.1.5",
    "zod": ">= 3.20.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": false
    },
    "cross-fetch": {
      "optional": false
    }
  }
}
