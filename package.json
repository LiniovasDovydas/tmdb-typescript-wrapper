{
  "name": "tmdb-typescript-wrapper",
  "type": "module",
  "version": "1.0.0",
  "description": "TypeScript wrapper for The Movie Database API",
  "main": "./lib/index.mjs",
  "module": "./lib/index.mjs",
  "repository": "https://github.com/LiniovasDovydas/tmdb-typescript-wrapper.git",
  "bugs": {
    "url": "https://github.com/LiniovasDovydas/tmdb-typescript-wrapper/issues",
    "email": "liniovas.dovydas@gmail.com"
  },
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch",
    "test": "vitest"
  },
  "keywords": [
    "tmdb",
    "typescript"
  ],
  "author": "Dovydas Liniovas",
  "license": "MIT",
  "exports": {
